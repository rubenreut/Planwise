# App Store Connect API Key Configuration
# Replace these with your actual values from App Store Connect

# The Key ID from App Store Connect
APP_STORE_CONNECT_API_KEY_ID=YOUR_KEY_ID_HERE

# The Issuer ID from App Store Connect  
APP_STORE_CONNECT_ISSUER_ID=YOUR_ISSUER_ID_HERE

# Path to your .p8 key file
APP_STORE_CONNECT_API_KEY_PATH=./fastlane/AuthKey_YOUR_KEY_ID.p8